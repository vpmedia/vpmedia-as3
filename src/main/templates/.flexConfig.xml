<flex-config xmlns="http://www.adobe.com/2006/flex-config">  
    <swf-version>${swf_version}</swf-version>
    <target-player>${player_version}</target-player> 
    <compiler>
        <define append="true"><name>APP::VERSION</name><value>"1.0.0"</value></define>
        <define append="true"><name>APP::DEBUG</name><value>true</value></define>
        <define append="true"><name>VIEWPORT::WIDTH</name><value>800</value></define>
        <define append="true"><name>VIEWPORT::HEIGHT</name><value>600</value></define>
        <define append="true"><name>VIEWPORT::SCALE</name><value>1</value></define>        
        <define append="true"><name>RUNTIME::AIR</name><value>true</value></define>    
        <define append="true"><name>PLATFORM::DESKTOP</name><value>true</value></define>
        <define append="true"><name>PLATFORM::MOBILE</name><value>false</value></define>
        <define append="true"><name>PLATFORM::WEB</name><value>false</value></define>
        <library-path append="true">
            <path-element>../libs</path-element> 
        </library-path>  
    </compiler>
    <metadata>
        <title>Docmet Commons</title>
        <description>Common Library</description>
        <creator>Andras Csizmadia</creator>
        <publisher>Docmet Systems</publisher>
        <language>EN</language>
    </metadata>
</flex-config>